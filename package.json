{
  "name": "women's-clothing-scm-synergy-system",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "tsx --env-file=.env.local server/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:bot": "npm -C tests test",
    "test:bot:style": "npm -C tests run test:style",
    "test:bot:pricing": "npm -C tests run test:pricing",
    "test:bot:anomaly": "npm -C tests run test:anomaly",
    "test:bot:restock": "npm -C tests run test:restock",
    "demo": "npm -C tests run demo",
    "demo:ui": "tsx tests/ui/demo-ui.ts"
  },
  "dependencies": {
    "@google/genai": "^1.38.0",
    "@supabase/supabase-js": "^2.47.0",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "iconv-lite": "^0.7.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@marp-team/marp-cli": "^4.2.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.1.2",
    "dotenv": "^17.2.3",
    "jsdom": "^27.4.0",
    "puppeteer": "^24.36.1",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^4.0.18"
  }
}
