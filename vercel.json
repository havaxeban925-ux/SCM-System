{
    "functions": {
        "api/index.ts": {
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api"
        },
        {
            "source": "/admin",
            "destination": "/admin/index.html"
        },
        {
            "source": "/admin/(.*)",
            "destination": "/admin/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}