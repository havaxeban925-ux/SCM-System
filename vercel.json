{
    "functions": {
        "api/[[...path]].ts": {
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/admin",
            "destination": "/admin/index.html"
        },
        {
            "source": "/admin/(.*)",
            "destination": "/admin/index.html"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ]
}